@article{li2021occlusionaware,
  title = {Occlusion-Aware on-Road Autonomous Driving: {{A}} Trajectory Planning Method Considering Occlusions of {{Lidars}}},
  shorttitle = {Occlusion-Aware on-Road Autonomous Driving},
  author = {Li, Bai},
