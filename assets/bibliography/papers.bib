@inproceedings{regolin2019path,
  title = {A {{Path Tracking Approach}} for {{Autonomous Driving}} on {{Slippery Surfaces}}},
  booktitle = {2019 {{IEEE International Conference}} on {{Connected Vehicles}} and {{Expo}} ({{ICCVE}})},
