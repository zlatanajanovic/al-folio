@article{li2020fast,
  title = {Fast {{Trajectory Planning}} in {{Cartesian}} Rather than {{Frenet Frame}}: {{A Precise Solution}} for {{Autonomous Driving}} in {{Complex Urban Scenarios}}},
  shorttitle = {Fast {{Trajectory Planning}} in {{Cartesian}} Rather than {{Frenet Frame}}},
  author = {Li, Bai and Zhang, Youmin},
  date = {2020-01-01},
  journaltitle = {IFAC-PapersOnLine},
  shortjournal = {IFAC-PapersOnLine},
  series = {21st {{IFAC World Congress}}},
  volume = {53},
