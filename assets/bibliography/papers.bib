@article{julio-rodriguez2022environment,
  title = {Environment {{Classification Using Machine Learning Methods}} for {{Eco-Driving Strategies}} in {{Intelligent Vehicles}}},
  author = {Julio-Rodr\'iguez, Jose del C. and Rojas-Ruiz, Carlos A. and Santana-D\'iaz, Alfredo and Bustamante-Bello, M. Rogelio and Ramirez-Mendoza, Ricardo A.},
