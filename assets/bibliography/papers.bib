@inproceedings{luijkx2022partnr,
  title = {{{PARTNR}}: {{Pick}} and Place {{Ambiguity Resolving}} by {{Trustworthy iNteractive leaRning}}},
  shorttitle = {{{PARTNR}}},
  booktitle = {{{NeurIPS}} 2022 {{Workshop}} on {{Robot Learning}}},
  author = {Luijkx, Jelle and Ajanovic, Zlatan and Ferranti, Laura and Kober, Jens},
