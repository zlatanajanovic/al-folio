@inproceedings{sun2020feedback,
  title = {Feedback {{Enhanced Motion Planning}} for {{Autonomous Vehicles}}},
  booktitle = {2020 {{IEEE}}/{{RSJ International Conference}} on {{Intelligent Robots}} and {{Systems}} ({{IROS}})},
