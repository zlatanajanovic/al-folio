  shorttitle = {Energy-{{Efficient Driving}} in {{Dynamic Environment}}},
  booktitle = {Advanced {{Microsystems}} for {{Automotive Applications}} 2017},
  author = {Ajanovi\'c, Zlatan and Stolz, Michael and Horn, Martin},
